{
  "name": "stack-overflow-react",
  "version": "1.0.0",
  "description": "a remake of stackoverflow front end in reactjs",
  "main": "app.js",
  "scripts": {
    "test": "jest --updateSnapshot --coverage",
    "build": "webpack && babel server.js --out-file dist/server.js",
    "start": "npm run build && node dist/server.js",
    "start:dev": "webpack-dev-server --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfJigsaw/stack-overflow-react.git"
  },
  "keywords": [
    "questions",
    "answers",
    "tags",
    "connect"
  ],
  "author": "professor jigsaw",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router-dom": "^4.3.1",
    "react-testing-library": "^5.3.2",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "./*/nightwatch.js",
      "public",
      "./*/store.js",
      "./*/mockData.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/ProfJigsaw/stack-overflow-react/issues"
  },
  "homepage": "https://github.com/ProfJigsaw/stack-overflow-react#readme"
}
